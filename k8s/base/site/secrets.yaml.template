# Site Secrets Template
# Copy this file to secrets.yaml and fill in the values
# DO NOT commit secrets.yaml to git - it's in .gitignore

apiVersion: v1
kind: Secret
metadata:
  name: clembot-site-secrets
type: Opaque
stringData:
  # Discord OAuth credentials for website login
  DISCORD_CLIENT_ID: "REPLACE_WITH_DISCORD_CLIENT_ID"
  DISCORD_CLIENT_SECRET: "REPLACE_WITH_DISCORD_CLIENT_SECRET"
  OAUTH_PERMISSIONS: "identify guilds"
